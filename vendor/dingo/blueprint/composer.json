{
    "name": "dingo/blueprint",
    "description": "API Blueprint documentation generator.",
    "keywords": ["blueprint", "api", "laravel", "dingo", "docs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Lewis",
            "email": "jason.lewis1991@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.5 || ^6.0",
        "illuminate/filesystem": "^5.5 || ^6.0",
        "doctrine/annotations": "~1.2",
        "phpdocumentor/reflection-docblock": "^3.1|^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0",
        "phpunit/phpunit": "^4.8.35 || ^5.4.3 || ^6.5"
    },
    "autoload": {
        "psr-4": {
            "Dingo\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dingo\\Blueprint\\Tests\\": "tests"
        }
    },
     "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
